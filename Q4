X=input("Enter name : "),int(input("Enter Your Age : "))
Y=input("Enter Another name : "),int(input("Enter Your Age : "))

def compare(X, Y):
    if Y < X:
        print (Y, "is Younger than", X) 
    else:
        print (X, "is Youngest than", Y)
       